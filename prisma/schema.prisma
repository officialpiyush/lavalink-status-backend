generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model stats {
  id                Int     @unique @default(autoincrement())
  name              String? @default("Lavalink Server")
  ip                String
  status            Int
  players           Int
  playing_players   Int
  uptime            BigInt
  cores             Int
  memory_usage      BigInt
  memory_reservable BigInt
  system_load       Decimal
  load              Decimal
}
