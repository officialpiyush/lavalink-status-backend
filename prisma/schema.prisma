generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Stats {
  id                Int     @unique @default(autoincrement())
  name              String? @default("Lavalink Server")
  ip                String
  status            Int
  players           Int
  playing_players   Int
  uptime            Int
  cores             Int
  memory_usage      Int
  memory_reservable Int
  system_load       Int
  load              Int
}
